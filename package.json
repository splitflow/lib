{
    "name": "@splitflow/lib",
    "version": "1.1.0",
    "description": "Splitflow lib package",
    "author": "team@splitflow.io",
    "homepage": "https://github.com/splitflow/lib#readme",
    "bugs": {
        "url": "https://github.com/splitflow/lib/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/splitflow/lib.git"
    },
    "type": "module",
    "files": [
        "dist/**/*.js",
        "dist/**/*.cjs",
        "dist/**/*.d.ts"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        },
        "./account": {
            "import": "./dist/account/index.js",
            "require": "./dist/account/index.cjs",
            "types": "./dist/account/index.d.ts"
        },
        "./design": {
            "import": "./dist/design/index.js",
            "require": "./dist/design/index.cjs",
            "types": "./dist/design/index.d.ts"
        },
        "./editor": {
            "import": "./dist/editor/index.js",
            "require": "./dist/editor/index.cjs",
            "types": "./dist/editor/index.d.ts"
        },
        "./orchestra": {
            "import": "./dist/orchestra/index.js",
            "require": "./dist/orchestra/index.cjs",
            "types": "./dist/orchestra/index.d.ts"
        },
        "./project": {
            "import": "./dist/project/index.js",
            "require": "./dist/project/index.cjs",
            "types": "./dist/project/index.d.ts"
        },
        "./style": {
            "import": "./dist/style/index.js",
            "require": "./dist/style/index.cjs",
            "types": "./dist/style/index.d.ts"
        },
        "./config": {
            "import": "./dist/config/index.js",
            "require": "./dist/config/index.cjs",
            "types": "./dist/config/index.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ],
            "account": [
                "dist/account/index.d.ts"
            ],
            "design": [
                "dist/design/index.d.ts"
            ],
            "editor": [
                "dist/editor/index.d.ts"
            ],
            "orchestra": [
                "dist/orchestra/index.d.ts"
            ],
            "project": [
                "dist/project/index.d.ts"
            ],
            "style": [
                "dist/style/index.d.ts"
            ],
            "config": [
                "dist/config/index.d.ts"
            ]
        }
    },
    "scripts": {
        "build": "tsup",
        "build:watch": "tsup --watch",
        "test": "vitest"
    },
    "devDependencies": {
        "tsup": "^6.5.0",
        "typescript": "^4.9.4",
        "vitest": "^0.27.3"
    },
    "dependencies": {
        "@splitflow/core": "^2.1.0"
    }
}
